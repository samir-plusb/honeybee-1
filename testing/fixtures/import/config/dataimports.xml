<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
    xmlns="http://berlinonline.de/schemas/midas/config/dataimport/definition/1.0"
    xmlns:env="http://berlinonline.de/schemas/midas/config/envelope/definition/1.0"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.1">
    <ae:configuration>
        <dataimports>

            <dataimport name="couchdb" class="CouchDbDataImport">
                <description>Imports data into a configured couchdb database.</description>
                <datasources>
                    <datasource>imperia</datasource>
                    <datasource>dpa</datasource>
                    <datasource>rss</datasource>
                </datasources>
                <settings>
                    <setting name="couchdb_database">CouchImport</setting>
                </settings>
            </dataimport>

            <dataimport name="workflow" class="WorkflowItemDataImport">
                <description>Imports data in form of IWorkflowItems.</description>
                <datasources>
                    <datasource>imperia</datasource>
                    <datasource>rss</datasource>
                    <datasource>dpa</datasource>
                    <datasource>imap</datasource>
                </datasources>
                <settings>
                    <setting name="notify">false</setting>
                </settings>
            </dataimport>

        </dataimports>
    </ae:configuration>
</ae:configurations>