                            <ae:parameter name="invalid_rev_text">Dieser Eintrag wurde zwischenzeitlich geändert und ist nicht mehr aktuell. Bitte laden sie Seite neu.</ae:parameter>
                            <ae:parameter name="release_ticket_error_text">Beim Versuch das Ticket freizugeben trat ein Fehler auf. Bitte versuchen sie es noch einmal.</ae:parameter>
                            <ae:parameter name="release_ticket_db_error">Datenbank Fehler</ae:parameter>
                            <ae:parameter name="release_ticket_unex_error">Unerwarteter Fehler</ae:parameter>
                            <ae:parameter name="ticket_not_avail">Ticket bereits vergeben</ae:parameter>
