        <workflow name="news">
            <description>The default workflow for refinining news.</description>
            <steps start="refine">
                <step id="refine" name="Initial refinement of imported news data.">
                    <plugin>RefineNews</plugin>
                    <ae:parameter></ae:parameter>
                    <gates>
                        <gate end="true">Hurray, I can haz news</gate>
                    </gates>
                </step>
            </steps>
        </workflow>
    </ae:configuration>
</ae:configurations>
