<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
    xmlns="http://agavi.org/agavi/config/parts/config_handlers/1.1"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.1">
    <ae:configuration>
        <handlers>
            <handler pattern="*.workflow.xml" class="WorkflowConfigHandler">
                <validation>%core.app_dir%/modules/Workflow/config/xsd/workflows.xsd</validation>
                <ae:parameter name="namespace_uri">http://berlinonline.de/schemas/midas/workflow/1.0</ae:parameter>
            </handler>
            <handler pattern="workflows.xml" class="WorkflowConfigHandler">
                <validation>%core.app_dir%/modules/Workflow/config/xsd/workflows.xsd</validation>
                <ae:parameter name="namespace_uri">http://berlinonline.de/schemas/midas/workflow/1.0</ae:parameter>
            </handler>
        </handlers>
    </ae:configuration>
</ae:configurations>
