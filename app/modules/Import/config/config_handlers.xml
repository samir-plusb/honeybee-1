<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0" xmlns="http://agavi.org/agavi/config/parts/config_handlers/1.0" parent="%core.system_config_dir%/config_handlers.xml">
    <ae:configuration>
        <handlers>

            <handler pattern="*/dataimports.xml" class="AgaviReturnArrayConfigHandler">
                <validation>%core.app_dir%/modules/Import/config/xsd/dataimport.xsd</validation>
                <ae:parameters>
                    <ae:parameter name="namespace_uri">http://berlinonline.de/schemas/midas/config/dataimport/definition/1.0</ae:parameter>
                </ae:parameters>
            </handler>
            
            <handler pattern="*/datasources.xml" class="AgaviReturnArrayConfigHandler">
                <validation>%core.app_dir%/modules/Import/config/xsd/datasource.xsd</validation>
                <ae:parameters>
                    <ae:parameter name="namespace_uri">http://berlinonline.de/schemas/midas/config/datasource/definition/1.0</ae:parameter>
                </ae:parameters>
            </handler>

        </handlers>
    </ae:configuration>

</ae:configurations>