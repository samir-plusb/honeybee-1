<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0" xmlns="http://agavi.org/agavi/config/parts/translation/1.0">
    <ae:configuration>
        <translators default_domain="user.errors">
            <translator domain="user">
                <message_translator class="AgaviSimpleTranslator">

                    <!--
                        Workflow state translations.
                    -->
                    <ae:parameter name="workflow">
                        <ae:parameter name="de">
                            <ae:parameter name="edit">Inaktiv</ae:parameter>
                            <ae:parameter name="publish">Aktivierung läuft</ae:parameter>
                            <ae:parameter name="published">Aktiv</ae:parameter>
                            <ae:parameter name="archived">Archiv</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        List translations for the User module go here.
                    -->
                    <ae:parameter name="list">
                        <ae:parameter name="de">
                            <!-- list header and standard action names -->
                            <ae:parameter name="created">Erstellt</ae:parameter>
                            <ae:parameter name="owner">Besitzer</ae:parameter>
                            <ae:parameter name="username">Nutzername</ae:parameter>
                            <ae:parameter name="firstname">Vorname</ae:parameter>
                            <ae:parameter name="lastname">Nachname</ae:parameter>
                            <ae:parameter name="actions">Aktionen</ae:parameter>
                            <ae:parameter name="promote">Publizieren</ae:parameter>
                            <ae:parameter name="demote">Depublizieren</ae:parameter>
                            <ae:parameter name="state">Workflow Schritt</ae:parameter>
                            <ae:parameter name="role">Rolle</ae:parameter>

                            <!-- reference batch dialogs -->
                            <ae:parameter name="batch_actions">Stapelverarbeitung</ae:parameter>
                            <ae:parameter name="override_references">bestehendende Verknüpfungen überschreiben</ae:parameter>
                            <ae:parameter name="append_reference">zu bestehenden Verknüpfungen hinzufügen</ae:parameter>
                            <ae:parameter name="searching">Suche läuft ...</ae:parameter>
                            <ae:parameter name="max_references_reached">maximal erlaubte Anzahl an Verknüpfungen erreicht</ae:parameter>
                            <ae:parameter name="reference_suggest_to_short">Bitte mindestens ein Zeichen eingeben</ae:parameter>
                            <ae:parameter name="no_references_found">Keine passenden Ergebnisse gefunden</ae:parameter>
                            <ae:parameter name="assign_references">Verknüpfen</ae:parameter>

                            <ae:parameter name="edit">Bearbeiten</ae:parameter>
                            <ae:parameter name="view">Ansehen</ae:parameter>
                            <ae:parameter name="disabled">Gesperrt</ae:parameter>
                            <ae:parameter name="delete">Löschen</ae:parameter>

                            <!-- workflow transitions (context sensitive action names for promote and demote) -->
                            <ae:parameter name="edit.promote">Aktivieren</ae:parameter>
                            <ae:parameter name="edit.delete">Löschen</ae:parameter>
                            <ae:parameter name="published.promote">Archivieren</ae:parameter>
                            <ae:parameter name="published.demote">Deaktivieren</ae:parameter>
                            <ae:parameter name="published.delete">Löschen</ae:parameter>
                            <ae:parameter name="archived.demote">Reimportieren</ae:parameter>
                            <ae:parameter name="archived.delete">Löschen</ae:parameter>

                            <!-- workflow transition prompts -->
                            <ae:parameter name="edit.promote.prompt">Sind sie sicher, dass sie diesen Datensatz publizieren (abnehmen) möchten?</ae:parameter>
                            <ae:parameter name="published.demote.prompt">Sind sie sicher, dass sie diesen Datensatz depublizieren möchten?</ae:parameter>
                            <ae:parameter name="edit.delete.prompt">Sind sie sicher, dass sie diesen Datensatz löschen möchten?</ae:parameter>
                            <ae:parameter name="published.delete.prompt">Sind sie sicher, dass sie diesen (publizierten)Datensatz löschen möchten?</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        Input translations for the User module go here.
                    -->
                    <!-- Fieldnames -->
                    <ae:parameter name="rendering.input.field">
                        <ae:parameter name="de">
                            <ae:parameter name="identifier">Identifikation</ae:parameter>
                            <ae:parameter name="revision">Revision</ae:parameter>
                            <ae:parameter name="username">Nutzername</ae:parameter>
                            <ae:parameter name="firstname">Vorname</ae:parameter>
                            <ae:parameter name="lastname">Nachname</ae:parameter>
                            <ae:parameter name="email">Email</ae:parameter>
                            <ae:parameter name="role">Rolle</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>
                    <!-- Group- and Tabnames -->
                    <ae:parameter name="rendering.input.document">
                        <ae:parameter name="de">
                            <ae:parameter name="edit-form-title">User erstellen/bearbeiten</ae:parameter>
                            <ae:parameter name="meta">Metadaten</ae:parameter>
                            <ae:parameter name="content">Inhalte</ae:parameter>
                            <ae:parameter name="main">Stammdaten</ae:parameter>
                            <ae:parameter name="privledges">Zugriffsrechte</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        Error translations for the User module go here.
                    -->
                    <ae:parameter name="errors">
                        <ae:parameter name="de">
                            <ae:parameter name="storage_error">Das speichern der (User) Daten schlug fehl.</ae:parameter>
                            <ae:parameter name="export_error">Während der Datenverarbeitung trat ein unerwarteter Fehler auf.</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <ae:parameter name="ui">
                        <ae:parameter name="de">
                            <ae:parameter name="Signin">Anmelden</ae:parameter>
                            <ae:parameter name="Set password">Kennwort setzen</ae:parameter>
                            <ae:parameter name="Username">Nutzername</ae:parameter>
                            <ae:parameter name="Reset password">Kennwort zurücksetzen</ae:parameter>
                            <ae:parameter name="Password">Kennwort</ae:parameter>
                            <ae:parameter name="Repeat password">Kennwort wiederholen</ae:parameter>
                            <ae:parameter name="Please sign in">Bitte Nutzernamen und Kennwort eingeben</ae:parameter>
                            <ae:parameter name="Please enter your email or username."><![CDATA[Bitte Nutzernamen <b>oder</b> Emailadresse eingeben.]]></ae:parameter>
                            <ae:parameter name="Min. %d characters long">Mind. %d Zeichen lang</ae:parameter>
                            <ae:parameter name="Min. %d lowercase letters">Mind. %d Kleinbuchstaben</ae:parameter>
                            <ae:parameter name="Min. %d uppercase characters">Mind. %d Grossbuchstaben</ae:parameter>
                            <ae:parameter name="Min. %d numeric characters">Mind. %d Ziffern</ae:parameter>
                            <ae:parameter name="special characters">Sonderzeichen</ae:parameter>
                            <ae:parameter name="numeric characters">Ziffern</ae:parameter>
                            <ae:parameter name="uppercase letters">Grossbuchstaben</ae:parameter>
                            <ae:parameter name="lowercase letters">Kleinbuchstaben</ae:parameter>
                            <ae:parameter name="weak">schwach</ae:parameter>
                            <ae:parameter name="medium">mittel</ae:parameter>
                            <ae:parameter name="strong">stark</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <ae:parameter name="messages">
                        <ae:parameter name="de">
                            <ae:parameter name="invalid_login">Die Kombination aus Nutzername und Kennwort ist ungültig.</ae:parameter>
                            <ae:parameter name="invalid_account">Ungültige Kontodaten; bitte die angegebene Email oder den Nutzernamen auf Richtigkeit überprüfen.</ae:parameter>
                            <ae:parameter name="Login Error">Fehler während der Anmeldung</ae:parameter>
                            <ae:parameter name="reset_success_title">Sie haben Post!</ae:parameter>
                            <ae:parameter name="reset_success_msg"><![CDATA[
                                <p>Wir haben eine Email an die Adresse verschickt, die mit dem angegebenen Nutzerkonto verküpft ist.</p>
                                <p>Darin finden sie die erforderlichen Schritte um ihr Passwort zurück zu setzen.</p>
                            ]]></ae:parameter>
                            <ae:parameter name="session_regeneration_error">Interner Fehler beim Login. Bitte versuchen Sie es später noch einmal.</ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="reset_success_title">You have mail!</ae:parameter>
                            <ae:parameter name="reset_success_msg"><![CDATA[
                                <p>The password for the affected user account may be reset now.</p>
                                <p>The necessary steps have been mailed to the email adress related to the latter user account.</p>
                            ]]></ae:parameter>
                            <ae:parameter name="session_regeneration_error">Internal error while login. Please try again later.</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>
                </message_translator>
            </translator>
        </translators>
    </ae:configuration>
</ae:configurations>
