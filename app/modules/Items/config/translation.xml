<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.1" xmlns="http://agavi.org/agavi/config/parts/translation/1.0">
    <ae:configuration>

        <translators default_domain="items.errors">
            <translator domain="items">
                <message_translator class="AgaviSimpleTranslator">

                    <ae:parameter name="structure">
                        <ae:parameter name="de">
                            <ae:parameter name="title">Titel</ae:parameter>
                            <ae:parameter name="source">Quelle</ae:parameter>
                            <ae:parameter name="timestamp">Eingangsdatum</ae:parameter>
                            <ae:parameter name="state">Status</ae:parameter>
                            <ae:parameter name="category">Rubrik</ae:parameter>
                            <ae:parameter name="district">Bezirk</ae:parameter>
                            <ae:parameter name="owner">Besitzer</ae:parameter>
                            <ae:parameter name="actions">Aktionen</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <ae:parameter name="news_workflow">
                        <ae:parameter name="de">
                            <ae:parameter name="refine_news">Neu</ae:parameter>
                            <ae:parameter name="publish_news">Publiziert</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>


                    <ae:parameter name="ui">
                        <ae:parameter name="de">
                            <ae:parameter name="Login">Anmeldung</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <ae:parameter name="dialogs">
                        <ae:parameter name="de">
                            <ae:parameter name="confirm_delete_title">Bitte bestätigen</ae:parameter>
                            <ae:parameter name="confirm_delete_text">dass der Eintrag wirklich gelöscht werden soll.</ae:parameter>
                            <ae:parameter name="confirm_delete">löschen</ae:parameter>
                            <ae:parameter name="deny_delete">abbrechen</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <ae:parameter name="errors">
                        <ae:parameter name="de">
                            <ae:parameter name="This combination of username and password is invalid.">Die Kombination von Benutzername und Kennwort ist ungültig.</ae:parameter>
                            <ae:parameter name="Failed authentication attempt for username %1$s, require group membership of '%2$s'">Fehlende Gruppe '%2$s' für Benutzer '%1$s'.</ae:parameter>
                            <ae:parameter name="Login Error">Fehler in der Anmeldung</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                </message_translator>

            </translator>
        </translators>

    </ae:configuration>

</ae:configurations>