<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.1" xmlns="http://agavi.org/agavi/config/parts/translation/1.0">
    <ae:configuration>

        <translators default_domain="shofi.errors">
            <translator domain="shofi">
                <message_translator class="AgaviSimpleTranslator">

                    <ae:parameter name="list">
                        <ae:parameter name="de">
                            <ae:parameter name="company">Firma</ae:parameter>
                            <ae:parameter name="name">Name</ae:parameter>
                            <ae:parameter name="product">Produkt</ae:parameter>
                            <ae:parameter name="category">Kategorie</ae:parameter>
                            <ae:parameter name="source">Primärquelle</ae:parameter>
                            <ae:parameter name="last_import_id">Updatequelle</ae:parameter>
                            <ae:parameter name="vertical">Leuchtturm</ae:parameter>
                            <ae:parameter name="firstName">Vorname</ae:parameter>
                            <ae:parameter name="lastName">Nachname</ae:parameter>
                            <ae:parameter name="street">Straße</ae:parameter>
                            <ae:parameter name="date">Erstellt</ae:parameter>
                            <ae:parameter name="step">Status</ae:parameter>
                            <ae:parameter name="owner">Besitzer</ae:parameter>
                            <ae:parameter name="actions">Aktionen</ae:parameter>
                            <ae:parameter name="edit">Bearbeiten</ae:parameter>
                            <ae:parameter name="promote">Publizieren</ae:parameter>
                            <ae:parameter name="assign_category">Kategorie zuweisen</ae:parameter>
                            <ae:parameter name="delete">Löschen</ae:parameter>
                            <ae:parameter name="no-product">Nicht verkauft</ae:parameter>
                            <ae:parameter name="buiseness">Geschäftseintrag</ae:parameter>
                            <ae:parameter name="premium">Premiumeintrag</ae:parameter>
                            <ae:parameter name="last_modified">Zuletzt geändert</ae:parameter>

                            <!-- ShofiTranslationRenderer mapped translations -->
                            <!-- datasources -->
                            <ae:parameter name="places-wkg">WKG</ae:parameter>
                            <ae:parameter name="hotels-gdoc">GoogleDocs (Hotels)</ae:parameter>
                            <ae:parameter name="hotels-btk">BTK (Hotels)</ae:parameter>
                            <ae:parameter name="restaurants-tip">TIP (Restaurants)</ae:parameter>
                            <ae:parameter name="theaters-telavision">Telavision (Kinos)</ae:parameter>
                            <ae:parameter name="places-eventx">EventX (Orte)</ae:parameter>
                            <!-- products -->
                            <ae:parameter name="premium">Premium Eintrag</ae:parameter>
                            <ae:parameter name="freemium">Freemium Eintrag</ae:parameter>
                            <ae:parameter name="business">Business Eintrag</ae:parameter>
                            <ae:parameter name="basic">Basis Eintrag</ae:parameter>
                            <ae:parameter name="no-product">-</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <ae:parameter name="errors">
                        <ae:parameter name="de">
                            <ae:parameter name="storage_error">Das speichern der Ortsdaten schlug fehl.</ae:parameter>
                            <ae:parameter name="geo_invalid">Lokalisierung fehlt.</ae:parameter>
                            <ae:parameter name="unexpected_error">Während der Datenverarbeitung trat ein unerwarteter Fehler auf.</ae:parameter>
                            <ae:parameter name="export_error">Frontend-Synchronisation fehlgeschlagen.</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                </message_translator>

                <translator domain="datetime">
                    <date_formatter>
                        <ae:parameters>
                            <ae:parameter name="type">datetime</ae:parameter>
                            <ae:parameter name="format">EEEE dd-MM-yyyy, HH:mm</ae:parameter>
                        </ae:parameters>
                    </date_formatter>
                </translator>

            </translator>
        </translators>

    </ae:configuration>

</ae:configurations>