<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
    xmlns="http://berlinonline.de/schemas/midas/config/dataimport/definition/1.0"
    xmlns:env="http://berlinonline.de/schemas/midas/config/envelope/definition/1.0"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0">
    <ae:configuration>
        <dataimports>

            <dataimport name="shofi.verticals" class="ShofiVerticalsDataImport">
                <description>Imports shofi-vertical data records directly into the workflow.</description>
                <datasources>
                    <datasource>verticals-catalog</datasource>
                </datasources>
                <settings>
                    <setting name="notify">true</setting>
                </settings>
            </dataimport>

        </dataimports>
    </ae:configuration>
</ae:configurations>