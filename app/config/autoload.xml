<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations 
    xmlns="http://agavi.org/agavi/config/parts/autoload/1.0" 
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0" 
    xmlns:xi="http://www.w3.org/2001/XInclude" 
    parent="%core.system_config_dir%/autoload.xml">
    <ae:configuration>
        <autoloads>
            <!-- config handlers -->
            <autoload name="ProjectDataImportConfigHandler">%core.lib_dir%/agavi/config_handler/ProjectDataImportConfigHandler.class.php</autoload>
            <autoload name="ProjectDataSourceConfigHandler">%core.lib_dir%/agavi/config_handler/ProjectDataSourceConfigHandler.class.php</autoload>
            <autoload name="ProjectBaseConfigHandler">%core.lib_dir%/agavi/config_handler/ProjectBaseConfigHandler.class.php</autoload>
            <autoload name="ProjectAclConfigHandler">%core.lib_dir%/agavi/config_handler/ProjectAclConfigHandler.class.php</autoload>
            <autoload name="WorkflowConfigHandler">%core.lib_dir%/agavi/config_handler/WorkflowConfigHandler.class.php</autoload>
            <!-- database stuff -->
            <autoload name="ElasticSearchDatabase">%core.lib_dir%/agavi/database/ElasticSearchDatabase.class.php</autoload>
            <autoload name="CouchDbDatabase">%core.lib_dir%/agavi/database/CouchDbDatabase.class.php</autoload>
            <autoload name="IDatabaseSetup">%core.lib_dir%/agavi/database/IDatabaseSetup.iface.php</autoload>
            <autoload name="ExtendedCouchDbClient">%core.lib_dir%/agavi/database/ExtendedCouchDbClient.class.php</autoload>
            <autoload name="CouchdbClientException">%core.lib_dir%/agavi/database/CouchdbClientException.class.php</autoload>
            <autoload name="CouchDbDatabaseSetup">%core.lib_dir%/agavi/database/CouchDbDatabaseSetup.class.php</autoload>
            <autoload name="ElasticSearchDatabaseSetup">%core.lib_dir%/agavi/database/ElasticSearchDatabaseSetup.class.php</autoload>
            
            <!-- 
                Rendering - Rendering GUI for documents and modules.
            -->
            <autoload name="IRenderer">%core.lib_dir%/renderer/IRenderer.iface.php</autoload>
            <autoload name="DocumentRenderer">%core.lib_dir%/renderer/document/DocumentRenderer.class.php</autoload>
            <autoload name="FieldRenderer">%core.lib_dir%/renderer/field/FieldRenderer.class.php</autoload>
            <autoload name="TextareaFieldInputRenderer">%core.lib_dir%/renderer/field/Input/TextareaFieldInputRenderer.class.php</autoload>
            <autoload name="FieldRendererFactory">%core.lib_dir%/renderer/field/FieldRendererFactory.class.php</autoload>
            <autoload name="FieldInputRenderer">%core.lib_dir%/renderer/field/Input/FieldInputRenderer.class.php</autoload>
            <autoload name="ReferenceFieldInputRenderer">%core.lib_dir%/renderer/field/Input/ReferenceFieldInputRenderer.class.php</autoload>
            <autoload name="DocumentInputRenderer">%core.lib_dir%/renderer/document/Input/DocumentInputRenderer.class.php</autoload>
            <autoload name="TextCollectionFieldInputRenderer">%core.lib_dir%/renderer/field/Input/TextCollectionFieldInputRenderer.class.php</autoload>
            <autoload name="ImageFieldInputRenderer">%core.lib_dir%/renderer/field/Input/ImageFieldInputRenderer.class.php</autoload>
            <autoload name="KeyValuesCollectionFieldInputRenderer">%core.lib_dir%/renderer/field/Input/KeyValuesCollectionFieldInputRenderer.class.php</autoload>
            <autoload name="DateFieldInputRenderer">%core.lib_dir%/renderer/field/Input/DateFieldInputRenderer.class.php</autoload>
            <autoload name="LocationInputRenderer">%core.lib_dir%/renderer/field/Input/LocationInputRenderer.class.php</autoload>
            <autoload name="KoReadOnlyFieldInputRenderer">%core.lib_dir%/renderer/field/Input/KoReadOnlyFieldInputRenderer.class.php</autoload>

            <!-- 
                Workflow 
            -->
            <autoload name="WorkflowManager">%core.lib_dir%/workflow/WorkflowManager.class.php</autoload>
            <autoload name="IWorkflowResource">%core.lib_dir%/workflow/IWorkflowResource.iface.php</autoload>
            <autoload name="IWorkflowTicket">%core.lib_dir%/workflow/IWorkflowTicket.iface.php</autoload>
            <autoload name="Workflow">%core.lib_dir%/workflow/Workflow.class.php</autoload>
            <autoload name="WorkflowException">%core.lib_dir%/workflow/WorkflowException.class.php</autoload>
            <autoload name="IWorkflowPlugin">%core.lib_dir%/workflow/plugin/IWorkflowPlugin.iface.php</autoload>
            <autoload name="WorkflowBasePlugin">%core.lib_dir%/workflow/plugin/WorkflowBasePlugin.class.php</autoload>
            <autoload name="WorkflowDeletePlugin">%core.lib_dir%/workflow/plugin/WorkflowDeletePlugin.class.php</autoload>
            <autoload name="WorkflowPublishPlugin">%core.lib_dir%/workflow/plugin/WorkflowPublishPlugin.class.php</autoload>
            <autoload name="WorkflowArchivePlugin">%core.lib_dir%/workflow/plugin/WorkflowArchivePlugin.class.php</autoload>
            <autoload name="WorkflowInteractivePlugin">%core.lib_dir%/workflow/plugin/WorkflowInteractivePlugin.class.php</autoload>
            <autoload name="WorkflowNullPlugin">%core.lib_dir%/workflow/plugin/WorkflowNullPlugin.class.php</autoload>
            <autoload name="IWorkflowPluginResult">%core.lib_dir%/workflow/plugin/IWorkflowPluginResult.iface.php</autoload>
            <autoload name="WorkflowPluginResult">%core.lib_dir%/workflow/plugin/WorkflowPluginResult.class.php</autoload>
            <autoload name="WorkflowInteractivePluginResult">%core.lib_dir%/workflow/plugin/WorkflowInteractivePluginResult.class.php</autoload>

            <!-- 
                third party face detection lib 
            -->
            <autoload name="Face_Detector">%core.lib_dir%/php-facedetection/FaceDetector.php</autoload>
        </autoloads>
    </ae:configuration>
    
    <xi:include href="%core.app_dir%/config/includes/autoload.xml"
        xpointer="xmlns(ae=http://agavi.org/agavi/config/global/envelope/1.0) xpointer(/ae:configurations/*)">
        <xi:fallback />
    </xi:include>
</ae:configurations>
