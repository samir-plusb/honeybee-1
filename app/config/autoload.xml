<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations 
    xmlns="http://agavi.org/agavi/config/parts/autoload/1.0" 
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0" 
    xmlns:xi="http://www.w3.org/2001/XInclude" 
    parent="%core.system_config_dir%/autoload.xml">
    <ae:configuration>
        <autoloads>
            <!-- Extending Agavi -->
            <autoload name="ProjectBaseAction">%core.lib_dir%/agavi/action/ProjectBaseAction.class.php</autoload>
            <autoload name="ProjectBaseModel">%core.lib_dir%/agavi/model/ProjectBaseModel.class.php</autoload>
            <autoload name="ProjectBaseView">%core.lib_dir%/agavi/view/ProjectBaseView.class.php</autoload>

            <autoload name="ProjectArrayValidator">%core.lib_dir%/agavi/validator/ProjectArrayValidator.class.php</autoload>

            <autoload name="ProjectLanguageRoutingCallback">%core.lib_dir%/agavi/routing/ProjectLanguageRoutingCallback.class.php</autoload>

            <autoload name="ProjectExecutionFilter">%core.lib_dir%/agavi/filter/ProjectExecutionFilter.class.php</autoload>
            <autoload name="ProjectResourcePacker">%core.lib_dir%/agavi/filter/ProjectResourcePacker.class.php</autoload>
            <autoload name="ProjectResourceFilter">%core.lib_dir%/agavi/filter/ProjectResourceFilter.class.php</autoload>
            <autoload name="ProjectResourceFilterConfig">%core.lib_dir%/agavi/filter/ProjectResourceFilterConfig.class.php</autoload>

            <autoload name="ProjectDataImportConfigHandler">%core.lib_dir%/agavi/config_handler/ProjectDataImportConfigHandler.class.php</autoload>
            <autoload name="ProjectDataSourceConfigHandler">%core.lib_dir%/agavi/config_handler/ProjectDataSourceConfigHandler.class.php</autoload>
            <autoload name="ProjectBaseConfigHandler">%core.lib_dir%/agavi/config_handler/ProjectBaseConfigHandler.class.php</autoload>
            <autoload name="ProjectAclConfigHandler">%core.lib_dir%/agavi/config_handler/ProjectAclConfigHandler.class.php</autoload>

            <autoload name="ElasticSearchDatabase">%core.lib_dir%/agavi/database/ElasticSearchDatabase.class.php</autoload>
            <autoload name="CouchDbDatabase">%core.lib_dir%/agavi/database/CouchDbDatabase.class.php</autoload>
            <autoload name="IDatabaseSetup">%core.lib_dir%/agavi/database/IDatabaseSetup.iface.php</autoload>
            <autoload name="ExtendedCouchDbClient">%core.lib_dir%/agavi/database/ExtendedCouchDbClient.class.php</autoload>
            <autoload name="CouchdbClientException">%core.lib_dir%/agavi/database/CouchdbClientException.class.php</autoload>
            <autoload name="CouchDbDatabaseSetup">%core.lib_dir%/agavi/database/CouchDbDatabaseSetup.class.php</autoload>
            <autoload name="ElasticSearchDatabaseSetup">%core.lib_dir%/agavi/database/ElasticSearchDatabaseSetup.class.php</autoload>

            <autoload name="ProjectZendAclSecurityUser">%core.lib_dir%/agavi/user/ProjectZendAclSecurityUser.class.php</autoload>
            <autoload name="UserIsItemOwnerAssertion">%core.lib_dir%/agavi/user/assertions/UserIsItemOwnerAssertion.class.php</autoload>
            <!-- /Extending Agavi -->

            <!-- DataObject -->
            <autoload name="IComparable">%core.lib_dir%/data_object/IComparable.iface.php</autoload>
            <autoload name="IDataObject">%core.lib_dir%/data_object/IDataObject.iface.php</autoload>
            <autoload name="IFreezable">%core.lib_dir%/data_object/IFreezable.iface.php</autoload>
            <autoload name="FreezableDataObject">%core.lib_dir%/data_object/FreezableDataObject.class.php</autoload>
            <autoload name="BaseDataObject">%core.lib_dir%/data_object/BaseDataObject.class.php</autoload>
            <autoload name="IDocument">%core.lib_dir%/data_object/IDocument.iface.php</autoload>
            <autoload name="BaseDocument">%core.lib_dir%/data_object/BaseDocument.class.php</autoload>
            <autoload name="IDocumentStore">%core.lib_dir%/data_object/IDocumentStore.iface.php</autoload>
            <autoload name="CouchDocumentStore">%core.lib_dir%/data_object/CouchDocumentStore.class.php</autoload>
            <autoload name="IItemLocation">%core.lib_dir%/data_object/IItemLocation.iface.php</autoload>
            <autoload name="ItemLocation">%core.lib_dir%/data_object/ItemLocation.class.php</autoload>
            <autoload name="GeoPoint">%core.lib_dir%/data_object/GeoPoint.class.php</autoload>
            <!-- /DataObject -->

            <!-- Iterator -->
            <autoload name="ProjectDirectoryRegexpIterator">%core.lib_dir%/iterator/ProjectDirectoryRegexpIterator.class.php</autoload>
            <!-- /Iterator -->

            <!-- Event -->
            <autoload name="IEvent">%core.lib_dir%/event/IEvent.iface.php</autoload>
            <autoload name="IEventProxy">%core.lib_dir%/event/IEventProxy.iface.php</autoload>
            <autoload name="ProjectEventProxy">%core.lib_dir%/event/ProjectEventProxy.class.php</autoload>
            <autoload name="ProjectEvent">%core.lib_dir%/event/ProjectEvent.class.php</autoload>
            <!-- /Event -->

            <!-- Http -->
            <autoload name="ProjectCurl">%core.lib_dir%/http/ProjectCurl.class.php</autoload>
            <!-- /Http -->
            
            <!-- Mail -->
            <autoload name="MimeMailParser">%core.lib_dir%/mail/mailparse.php</autoload>
            <autoload name="MimeMailParser_attachment">%core.lib_dir%/mail/attachment.class.php</autoload>
            <!-- /Mail -->
            
            <autoload name="BaseExportDocumentFactory">%core.lib_dir%/BaseExportDocumentFactory.class.php</autoload>
            <autoload name="ProjectIdSequenceMap">%core.lib_dir%/data_object/ProjectIdSequenceMap.class.php</autoload>
            <autoload name="ProjectIdSequence">%core.lib_dir%/data_object/ProjectIdSequence.class.php</autoload>

            <!-- Config -->
            <autoload name="ConfigurationScanner">%core.lib_dir%/config/ConfigurationScanner.class.php</autoload>
            <autoload name="DefaultConfigGenerator">%core.lib_dir%/config/DefaultConfigGenerator.class.php</autoload>
            <autoload name="RoutingConfigGenerator">%core.lib_dir%/config/RoutingConfigGenerator.class.php</autoload>
            <!-- /Config -->
        </autoloads>
    </ae:configuration>
    
    <xi:include href="%core.app_dir%/config/includes/autoload.xml"
        xpointer="xmlns(ae=http://agavi.org/agavi/config/global/envelope/1.0) xpointer(/ae:configurations/*)">
        <xi:fallback />
    </xi:include>
</ae:configurations>
