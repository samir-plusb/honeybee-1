{
    "name": "berlinonline/honeybee",
    "type": "library",
    "description": "Framework for building content management systems.",
    "keywords": ["cms", "cmf" , "scafolding"],
    "homepage": "https://github.com/berlinonline/honeybee",
    "license": "MIT",
    "authors": [
        {
            "name": "Thorsten Schmitt-Rink"
        },
        {
            "name": "Leon Weidauer"
        },
        {
            "name": "Steffen Gransow"
        },
        {
            "name": "Tom Anheyer"
        }
    ],

    "autoload": {
        "psr-0": {
            "Honeybee" : "app/lib",
            "Testing" : "testing/tests"
        }
    },

    "require": {
        "php": ">=5.3.3",
        "agavi/agavi": "1.0.*",
        "phing/phing": "2.4.12",
        "berlinonline/dat0r": "dev-aggregates",
        "ruflin/Elastica": "v0.20.5.0.RC1",
        "zendframework/zend-permissions-acl": "2.2.0",
        "guzzle/guzzle": "~3.1",
        "swiftmailer/swiftmailer": "v4.3.1",
        "twig/twig": "1.*",
        "craur/craur": "1.6.*",
        "imagine/Imagine": "0.3.*",
        "jquery/jquery": "1.8.2",
        "allmarkedup/jquery-url-parser": "2.2.1",
        "farhadi/html5sortable": "0.9",
        "knockoutjs/knockoutjs": "2.2.1",
        "underscorejs/underscorejs": "1.4.2",
        "kineticjs/kineticjs": "4.5.1",
        "ivaynberg/select2": "3.3.2",
        "PhpDebugToolbar/php-debug-toolbar": "dev-master",
        "fzaninotto/faker": "v1.1.0",
        "sami/sami": "1.0",
        "psr/log": "1.0.0",
        "monolog/monolog": "1.5"
    },

    "require-dev": {
        "shrink/phpmd": "1.4.3",
        "shrink/phploc": "1.7.6",
        "pdepend/pdepend": "1.1.0",
        "zerkalica/php-code-browser": "dev-master",
        "squizlabs/php_codesniffer": "~1.4.4",
        "phpunit/phpunit": "3.7.*",
        "sebastian/phpcpd": "dev-master"
    },

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "agavi/agavi",
                "version": "1.0.7",
                "source": {
                    "url": "http://svn.agavi.org/",
                    "type": "svn",
                    "reference": "tags/1.0.7/"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "craur/craur",
                "version": "1.6.0",
                "source": {
                    "url": "https://github.com/DracoBlue/Craur.git",
                    "type": "git",
                    "reference": "f4f0bd435ef512b02b6251b1367e9b5103d09926"
                },
                "autoload": {
                    "classmap": ["php/Craur.class.php"]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "PhpDebugToolbar/php-debug-toolbar",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/graste/PhpDebugToolbar.git",
                    "type": "git",
                    "reference": "63b1b518e559b7e2995cda91a8edb707048c79a6"
                },
                "autoload": {
                    "classmap": ["lib/agavi/", "PhpDebugToolbar.class.php"]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.8.2",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.8.2.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/easing",
                "version": "1.3",
                "dist": {
                    "url": "http://gsgd.co.uk/sandbox/jquery/easing/jquery.easing.1.3.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "allmarkedup/jquery-url-parser",
                "version": "2.2.1",
                "source": {
                    "url": "https://github.com/allmarkedup/jQuery-URL-Parser.git",
                    "type": "git",
                    "reference": "69a50b31c1f6185731c1fca6fe9f31d58d6947f1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "knockoutjs/knockoutjs",
                "version": "2.2.1",
                "dist": {
                    "url": "http://knockoutjs.com/downloads/knockout-2.2.1.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "underscorejs/underscorejs",
                "version": "1.4.2",
                "dist": {
                    "url": "http://underscorejs.org/underscore-min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "kineticjs/kineticjs",
                "version": "4.5.1",
                "dist": {
                    "url": "http://d3lp1msu2r81bx.cloudfront.net/kjs/js/lib/kinetic-v4.5.1.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ivaynberg/select2",
                "version": "3.3.2",
                "source": {
                    "url": "https://github.com/ivaynberg/select2.git",
                    "type": "git",
                    "reference": "9697ae1e7fdf75bfd421d4e5fbd8d9b3ef8ec58b"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "farhadi/html5sortable",
                "version": "0.9",
                "source": {
                    "url": "https://github.com/farhadi/html5sortable.git",
                    "type": "git",
                    "reference": "b90b6baac95cdc0df1ec1682b7181ff94b1f7820"
                }
            }
        }
    ]
}
