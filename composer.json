{
    "name": "berlinonline/honeybee",
    "type": "library",
    "description": "Framework for building content management systems.",
    "keywords": ["cms", "cmf" , "scafolding"],
    "homepage": "https://github.com/berlinonline/honeybee",
    "license": "MIT",
    "authors": [
        {
            "name": "Leon Weidauer",
            "name": "Steffen Gransow",
            "name": "Thorsten Schmitt-Rink",
            "name": "Tom Anheyer"
        }
    ],

    "autoload": {
        "psr-0": {"Honeybee" : "app/lib" }
    },

    "require": {
        "php": ">=5.3.3",
        "agavi/agavi": "1.0.*",
        "phing/phing": "2.4.12",
        "berlinonline/dat0r": "dev-master",
        "ruflin/Elastica": "v0.20.5.0.RC1",
        "zendframework/zend-permissions-acl": "2.1.2",
        "guzzle/guzzle": "~3.1",
        "swiftmailer/swiftmailer": "v4.3.1",
        "twig/twig": "1.*",
        "craur/craur": "1.6.*",
        "imagine/Imagine": "0.3.*",
        "jquery/jquery": "1.8.2",
        "allmarkedup/jquery-url-parser": "2.2.1",
        "farhadi/html5sortable": "0.9",
        "knockoutjs/knockoutjs": "2.2.0",
        "underscorejs/underscorejs": "1.4.2",
        "kineticjs/kineticjs": "4.0.*",
        "twitter/bootstrap": "2.2.2",
        "fortawesome/font-awesome": "3.0.2",
        "ivaynberg/select2": "3.3.2",
        "PhpDebugToolbar/php-debug-toolbar": "1.5.*"
    },

    "minimum-stability": "dev",

    "require-dev": {
        "phpdocumentor/phpdocumentor": "dev-develop",
        "phpmd/phpmd": "1.4.1",
        "phploc/phploc": "dev-master",
        "pdepend/pdepend": "1.1.0",
        "zerkalica/php-code-browser": "dev-master",
        "squizlabs/php_codesniffer": "~1.4.4",
        "phpunit/phpunit": "3.7.*",
        "shrink/phpcpd": "dev-master"
    },

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "agavi/agavi",
                "version": "1.0.7",
                "source": {
                    "url": "http://svn.agavi.org/",
                    "type": "svn",
                    "reference": "tags/1.0.7/"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "craur/craur",
                "version": "1.6.0",
                "source": {
                    "url": "https://github.com/DracoBlue/Craur.git",
                    "type": "git",
                    "reference": "f4f0bd435ef512b02b6251b1367e9b5103d09926"
                },
                "autoload": {
                    "classmap": ["php/Craur.class.php"]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "PhpDebugToolbar/php-debug-toolbar",
                "version": "1.5.0",
                "source": {
                    "url": "https://github.com/DracoBlue/PhpDebugToolbar.git",
                    "type": "git",
                    "reference": "8f829a838dc6dee69dbbc4a4af28295bf07abd0a"
                },
                "autoload": {
                    "classmap": ["lib/agavi/", "PhpDebugToolbar.class.php"]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.8.2",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.8.2.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/easing",
                "version": "1.3",
                "dist": {
                    "url": "http://gsgd.co.uk/sandbox/jquery/easing/jquery.easing.1.3.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "allmarkedup/jquery-url-parser",
                "version": "2.2.1",
                "source": {
                    "url": "https://github.com/allmarkedup/jQuery-URL-Parser.git",
                    "type": "git",
                    "reference": "69a50b31c1f6185731c1fca6fe9f31d58d6947f1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "twitter/bootstrap",
                "version": "2.2.2",
                "source": {
                    "url": "https://github.com/twitter/bootstrap.git",
                    "type": "git",
                    "reference": "9376a7c221a64c2bf508d02ea2ccd85748d10fcc"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fortawesome/font-awesome",
                "version": "3.0.2",
                "source": {
                    "url": "https://github.com/FortAwesome/Font-Awesome.git",
                    "type": "git",
                    "reference": "0ad5e104960bb6051d4ec49abf42be4b74678e48"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "knockoutjs/knockoutjs",
                "version": "2.2.0",
                "dist": {
                    "url": "http://cloud.github.com/downloads/SteveSanderson/knockout/knockout-2.2.0.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "underscorejs/underscorejs",
                "version": "1.4.2",
                "dist": {
                    "url": "http://underscorejs.org/underscore-min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "kineticjs/kineticjs",
                "version": "4.0.5",
                "dist": {
                    "url": "http://kineticjs.com/download/v4.0.5/kinetic-v4.0.5.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ivaynberg/select2",
                "version": "3.3.2",
                "source": {
                    "url": "https://github.com/ivaynberg/select2.git",
                    "type": "git",
                    "reference": "9697ae1e7fdf75bfd421d4e5fbd8d9b3ef8ec58b"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "farhadi/html5sortable",
                "version": "0.9",
                "source": {
                    "url": "https://github.com/farhadi/html5sortable.git",
                    "type": "git",
                    "reference": "b90b6baac95cdc0df1ec1682b7181ff94b1f7820"
                }
            }
        }
    ]
}
