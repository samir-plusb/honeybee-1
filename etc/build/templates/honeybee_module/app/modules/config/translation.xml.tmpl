<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0" xmlns="http://agavi.org/agavi/config/parts/translation/1.0">
    <ae:configuration>
        <translators default_domain="%%MODULE_PREFIX%%.errors">
            <translator domain="%%MODULE_PREFIX%%">
                <message_translator class="AgaviSimpleTranslator">

                    <!--
                        Workflow state translations for the %%MODULE_NAME%% module.
                    -->
                    <ae:parameter name="workflow">
                        <ae:parameter name="de">
                            <ae:parameter name="edit">In Bearbeitung</ae:parameter>
                            <ae:parameter name="publish">Publikation läuft</ae:parameter>
                            <ae:parameter name="published">Publiziert</ae:parameter>
                            <ae:parameter name="archived">Archiv</ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="edit">Edit</ae:parameter>
                            <ae:parameter name="publish">Publishing</ae:parameter>
                            <ae:parameter name="published">Published</ae:parameter>
                            <ae:parameter name="archived">Archived</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        List translations for the %%MODULE_NAME%% module go here.
                    -->
                    <ae:parameter name="list">
                        <ae:parameter name="de">
                            <!-- list header and standard action names -->
                            <ae:parameter name="created">Erstellt</ae:parameter>
                            <ae:parameter name="owner">Besitzer/in</ae:parameter>
                            <ae:parameter name="actions">Aktionen</ae:parameter>
                            <ae:parameter name="promote">Publizieren</ae:parameter>
                            <ae:parameter name="demote">Depublizieren</ae:parameter>
                            <ae:parameter name="state">Workflow-Schritt</ae:parameter>
                            <ae:parameter name="edit">Bearbeiten</ae:parameter>
                            <ae:parameter name="delete">Löschen</ae:parameter>

                            <!-- reference batch dialogs -->
                            <ae:parameter name="batch_actions">Stapelverarbeitung</ae:parameter>
                            <ae:parameter name="override_references">Bestehendende Verknüpfungen überschreiben.</ae:parameter>
                            <ae:parameter name="append_reference">Zu bestehenden Verknüpfungen hinzufügen.</ae:parameter>
                            <ae:parameter name="searching">Suche läuft...</ae:parameter>
                            <ae:parameter name="max_references_reached">Maximal erlaubte Anzahl an Verknüpfungen erreicht.</ae:parameter>
                            <ae:parameter name="reference_suggest_to_short">Bitte mindestens ein Zeichen eingeben.</ae:parameter>
                            <ae:parameter name="no_references_found">Keine passenden Ergebnisse gefunden.</ae:parameter>
                            <ae:parameter name="assign_references">Verknüpfen</ae:parameter>

                            <!-- workflow transitions (context sensitive action names for promote and demote) -->
                            <ae:parameter name="edit.promote">Publizieren</ae:parameter>
                            <ae:parameter name="edit.delete">Löschen</ae:parameter>
                            <ae:parameter name="published.promote">Archivieren</ae:parameter>
                            <ae:parameter name="published.demote">Depublizieren</ae:parameter>
                            <ae:parameter name="published.delete">Löschen</ae:parameter>
                            <ae:parameter name="archived.demote">Reimportieren</ae:parameter>
                            <ae:parameter name="archived.delete">Löschen</ae:parameter>

                            <!-- workflow transition prompts -->
                            <ae:parameter name="edit.promote.prompt">Sind sie sicher, dass sie diesen Datensatz publizieren (abnehmen) möchten?</ae:parameter>
                            <ae:parameter name="published.demote.prompt">Sind sie sicher, dass sie diesen Datensatz depublizieren möchten?</ae:parameter>
                            <ae:parameter name="edit.delete.prompt">Sind sie sicher, dass sie diesen Datensatz löschen möchten?</ae:parameter>
                            <ae:parameter name="published.delete.prompt">Sind sie sicher, dass sie diesen (publizierten) Datensatz löschen möchten?</ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <!-- list header and standard action names -->
                            <ae:parameter name="created">Created</ae:parameter>
                            <ae:parameter name="owner">Owner</ae:parameter>
                            <ae:parameter name="actions">Actions</ae:parameter>
                            <ae:parameter name="promote">Publicise</ae:parameter>
                            <ae:parameter name="demote">Depublicise</ae:parameter>
                            <ae:parameter name="state">Workflow Step</ae:parameter>
                            <ae:parameter name="edit">Edit</ae:parameter>
                            <ae:parameter name="delete">Delete</ae:parameter>

                            <!-- reference batch dialogs -->
                            <ae:parameter name="batch_actions">Batch Actions</ae:parameter>
                            <ae:parameter name="override_references">Override existing references.</ae:parameter>
                            <ae:parameter name="append_reference">Append to existing references.</ae:parameter>
                            <ae:parameter name="searching">Searching...</ae:parameter>
                            <ae:parameter name="max_references_reached">Maximum number of references reached.</ae:parameter>
                            <ae:parameter name="reference_suggest_to_short">Please enter at least one character.</ae:parameter>
                            <ae:parameter name="no_references_found">No matching references found.</ae:parameter>
                            <ae:parameter name="assign_references">Assign reference.</ae:parameter>

                            <ae:parameter name="edit">Edit</ae:parameter>
                            <ae:parameter name="view">View</ae:parameter>
                            <ae:parameter name="disabled">Disabled</ae:parameter>
                            <ae:parameter name="delete">Delete</ae:parameter>

                            <!-- workflow transitions (context sensitive action names for promote and demote) -->
                            <ae:parameter name="edit.promote">Activate</ae:parameter>
                            <ae:parameter name="edit.delete">Delete</ae:parameter>
                            <ae:parameter name="published.promote">Archive</ae:parameter>
                            <ae:parameter name="published.demote">Deactivate</ae:parameter>
                            <ae:parameter name="published.delete">Delete</ae:parameter>
                            <ae:parameter name="archived.demote">Reimport</ae:parameter>
                            <ae:parameter name="archived.delete">Delete</ae:parameter>

                            <!-- workflow transition prompts -->
                            <ae:parameter name="edit.promote.prompt">Are you sure you want to approve and publicise the dataset?</ae:parameter>
                            <ae:parameter name="published.demote.prompt">Are you sure you want to depublicise the dataset?</ae:parameter>
                            <ae:parameter name="edit.delete.prompt">Are you sure you want to delete the dataset?</ae:parameter>
                            <ae:parameter name="published.delete.prompt">Are you sure to delete this publicised dataset?</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        Input translations for the %%MODULE_NAME%% module go here.
                    -->
                    <!-- Fieldnames -->
                    <ae:parameter name="rendering.input.field">
                        <ae:parameter name="de">
                            <ae:parameter name="identifier">Identifikation</ae:parameter>
                            <ae:parameter name="revision">Revision</ae:parameter>
                            <ae:parameter name="uuid">Uuid</ae:parameter>
                            <ae:parameter name="language">Sprache</ae:parameter>
                            <ae:parameter name="version">Version</ae:parameter>
                            <ae:parameter name="shortId">Kurze Id</ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="identifier">Identifier</ae:parameter>
                            <ae:parameter name="revision">Revision</ae:parameter>
                            <ae:parameter name="uuid">Uuid</ae:parameter>
                            <ae:parameter name="language">Language</ae:parameter>
                            <ae:parameter name="version">Version</ae:parameter>
                            <ae:parameter name="shortId">Short Id</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>
                    <!-- Group- and Tabnames -->
                    <ae:parameter name="rendering.input.document">
                        <ae:parameter name="de">
                            <ae:parameter name="edit-form-title">%%MODULE_NAME%% erstellen/bearbeiten</ae:parameter>
                            <ae:parameter name="meta">Metadaten</ae:parameter>
                            <ae:parameter name="content">Inhalte</ae:parameter>
                            <ae:parameter name="main">Stammdaten</ae:parameter>
                        </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="edit-form-title">%%MODULE_NAME%% create/modify</ae:parameter>
                            <ae:parameter name="meta">Metadata</ae:parameter>
                            <ae:parameter name="content">Content</ae:parameter>
                            <ae:parameter name="main">Main</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                    <!--
                        Error translations for the %%MODULE_NAME%% module go here.
                    -->
                    <ae:parameter name="errors">
                        <ae:parameter name="de">
                            <ae:parameter name="storage_error">Das Speichern der Daten schlug fehl.</ae:parameter>
                            <ae:parameter name="export_error">Während der Datenverarbeitung trat ein unerwarteter Fehler auf.</ae:parameter>
                            <ae:parameter name="An error occurred. Please check the given arguments.">Ein Fehler ist aufgetreten. Bitte kontrollieren sie die übergebenen Argumente.</ae:parameter>
                            <ae:parameter name="There is no such document.">Das Dokument existiert nicht.</ae:parameter>
                            <ae:parameter name="The value '%value$s' given for field '%fieldname$s' does not comply.">Der für das Feld '%fieldname$s' angegebene Wert '%value$s' ist nicht erlaubt.</ae:parameter>
                            <ae:parameter name="Missing required value for field '%fieldname$s'.">Bitte geben sie einen Wert für das Pflichtfeld '%fieldname$s' an.</ae:parameter>
                            <ae:parameter name="Missing value for field '%s'.">Wert für Feld '%s' fehlt.</ae:parameter>
                            <ae:parameter name="An invalid gate was given.">Der angegebene Workflowschritt ist nicht möglich.</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>
                        <ae:parameter name="en">
                            <ae:parameter name="storage_error">Storing the (%%MODULE_NAME%%) data failed.</ae:parameter>
                            <ae:parameter name="export_error">An error occurred while exporting the data.</ae:parameter>
                            <ae:parameter name="An invalid gate was given.">An invalid workflow step transition was given.</ae:parameter>
                        </ae:parameter>
                    </ae:parameter>

                </message_translator>
            </translator>
        </translators>
    </ae:configuration>
</ae:configurations>
